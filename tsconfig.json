{
	"extends": "@sapphire/ts-config/extra-strict",
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "ESNext",
		"target": "ESNext",
		"allowUnreachableCode": true,
		"emitDecoratorMetadata": false,
		"noEmit": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"outDir": "../dist",
		"rootDir": "src",
		"baseUrl": "src",
		"paths": {
			"#root/*": ["*"],
			"#lib/*": ["lib/*"],
			"#util/*": ["lib/util/*"]
		}
	},
	"include": ["src"]
}
