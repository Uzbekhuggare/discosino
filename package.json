{
	"name": "discosino",
	"version": "1.0.0",
	"description": "Casino bot for Discord",
	"author": "Henning Blomqvist (https://github.com/hblomqvist)",
	"license": "Apache-2.0",
	"repository": "github:hblomqvist/discosino",
	"private": true,
	"type": "module",
	"main": "./dist/discosino.js",
	"imports": {
		"#util/*": "./dist/lib/util/*.js",
		"#util/sanitizer": "./dist/lib/util/sanitizer/index.js",
		"#util/structures": "./dist/lib/util/structures/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/eval": "./dist/lib/eval/index.js",
		"#lib/framework": "./dist/lib/framework/index.js",
		"#config": "./dist/config.js"
	},
	"scripts": {
		"start": "node --enable-source-maps --experimental-specifier-resolution=node .",
		"start:migrate": "prisma migrate deploy && yarn start",
		"dev": "tsup --onSuccess \"yarn start\"",
		"watch": "yarn dev --watch",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "eslint --fix .",
		"format": "prettier --write .",
		"clean": "rimraf dist",
		"reinstall": "rimraf node_modules yarn.lock && yarn install",
		"postinstall": "node scripts/postinstall.js"
	},
	"dependencies": {
		"@prisma/client": "^4.1.0",
		"@sapphire/decorators": "^4.3.8",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.3",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.7.0",
		"discord.js": "^13.9.0",
		"dotenv": "^16.0.1",
		"envalid": "^7.3.1",
		"prettier": "^2.7.1",
		"prisma": "^4.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.0.6",
		"@types/prettier": "^2.6.3",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier-plugin-organize-imports": "^3.0.0",
		"prettier-plugin-prisma": "^4.1.0",
		"prettier-plugin-sh": "^0.12.6",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4"
	},
	"resolutions": {
		"@discordjs/builders": "0.16.0",
		"discord-api-types": "0.33.3"
	},
	"engines": {
		"node": ">=18"
	},
	"volta": {
		"node": "18.5.0",
		"yarn": "1.22.19"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@3.2.1"
}
