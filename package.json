{
	"name": "discosino",
	"version": "1.0.0",
	"description": "Casino bot for Discord",
	"author": "Henning Blomqvist (https://github.com/hblomqvist)",
	"license": "Apache-2.0",
	"repository": "github:hblomqvist/discosino",
	"private": true,
	"type": "module",
	"main": "./dist/discosino.js",
	"imports": {
		"#util/*": "./dist/lib/util/*.js",
		"#util/sanitizer": "./dist/lib/util/sanitizer/index.js",
		"#util/structures": "./dist/lib/util/structures/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/env": "./dist/lib/env/index.js",
		"#lib/eval": "./dist/lib/eval/index.js",
		"#lib/framework": "./dist/lib/framework/index.js",
		"#config": "./dist/config.js"
	},
	"scripts": {
		"start": "node --enable-source-maps --experimental-specifier-resolution=node .",
		"dev": "tsup --onSuccess \"yarn start\"",
		"watch": "tsup --watch --onSuccess \"yarn start\"",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "eslint --fix .",
		"format": "prettier --write .",
		"clean": "rimraf dist",
		"nuke": "rimraf node_modules yarn.lock",
		"postinstall": "node scripts/postinstall.js"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"discord.js": "^13.7.0",
		"dotenv": "^16.0.1",
		"prettier": "^2.6.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.1",
		"@commitlint/config-conventional": "^17.0.0",
		"@sapphire/eslint-config": "^4.3.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^17.0.36",
		"@types/prettier": "^2.6.3",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.30.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^12.4.2",
		"prettier-plugin-organize-imports": "^2.3.4",
		"prettier-plugin-prisma": "^3.14.0",
		"prettier-plugin-sh": "^0.11.0",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"tsup": "^6.0.1",
		"typescript": "^4.7.2"
	},
	"engines": {
		"node": ">=18"
	},
	"volta": {
		"node": "18.2.0",
		"yarn": "1.22.18"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@3.2.1"
}
