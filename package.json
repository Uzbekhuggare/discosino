{
	"name": "discosino",
	"version": "1.0.0",
	"description": "Casino bot for Discord",
	"author": "Henning Blomqvist (https://github.com/hblomqvist)",
	"license": "Apache-2.0",
	"repository": "github:hblomqvist/discosino",
	"private": true,
	"type": "module",
	"main": "./dist/discosino.js",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#lib/env": "./dist/lib/env/index.js",
		"#lib/eval": "./dist/lib/eval/index.js",
		"#lib/framework": "./dist/lib/framework/index.js",
		"#util/*": "./dist/lib/util/*.js",
		"#util/sanitizer": "./dist/lib/util/sanitizer/index.js",
		"#util/structures": "./dist/lib/util/structures/index.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"start": "node --enable-source-maps --experimental-specifier-resolution=node .",
		"dev": "node --enable-source-maps --experimental-specifier-resolution=node --require dotenv/config .",
		"watch": "tsup --watch --onSuccess \"yarn dev\"",
		"build": "tsup",
		"typecheck": "tsc -b",
		"lint": "eslint --fix --ext .ts src/",
		"format": "prettier --write .",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.6.2",
		"discord.js": "^13.6.0",
		"prettier": "^2.6.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.3.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^17.0.23",
		"@types/prettier": "^2.4.4",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.12.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier-plugin-organize-imports": "^2.3.4",
		"prettier-plugin-prisma": "^3.12.0",
		"prettier-plugin-sh": "^0.10.0",
		"pretty-quick": "^3.1.3",
		"tsup": "^5.12.4",
		"typescript": "^4.6.3"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"volta": {
		"node": "16.14.2",
		"yarn": "1.22.18"
	},
	"lint-staged": {
		"*.ts": "eslint --fix"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@3.2.0"
}
